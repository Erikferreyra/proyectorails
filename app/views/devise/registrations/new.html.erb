<div class="container">
  <!-- Content here -->
  <div class="be-blue white">
  <h1>Nuevo Usuario</h1>
  </div>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
  <div class="field">
    <%= f.label :Nombre %><br />
    <%= f.text_field :nombre, autofocus: true, autocomplete: "Nombre" %>
  </div>

   <div class="field">
    <%= f.label :Apellido %><br />
    <%= f.text_field :apellido, autofocus: true, autocomplete: "Apellido" %>
  </div>



  <div class="field">
    <%= f.label :Fecha_de_nacimiento%><br />
    <%= f.date_select :fechanac, start_year: 1908, end_year: 2000 %>
  </div>

  <div class="field">
    <%= f.label :Correo_electronico %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "Correo electronico" %>
  </div>

  <div class="field">
    <%= f.label :Numero_de_tarjeta %><br />
    <%= f.text_field :numtarjeta, autofocus: true, autocomplete: "Numero de tarjeta" %>
  </div>

  <div class="field">
    <%= f.label :Titular_de_tarjeta %><br />
    <%= f.text_field :titulartarjeta,autofocus: true, autocomplete: "Titular de tarjeta" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>
