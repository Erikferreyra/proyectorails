<div class="row center-xs">
  <div class="col-xs-30 col-md-20">
    <div class="box text-center">
        <h1>Iniciar sesion </h1>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="field">
            <%= f.label :Correo_electronico %><br />
            <%= f.email_field :email, autofocus: true,autocomplete: "Correo electronico" %>
          </div>

          <div class="field">
            <%= f.label :Contraseña %><br />
            <%= f.password_field :password, autocomplete: "Contraseña" %>
          </div>

          <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :Recordar_contraseña %>
            </div>
          <% end -%>

          <div class="actions">
            <%= f.submit "Entrar" %>
          </div>
        <% end %>

        <%= render "devise/shared/links" %>
    </div>
  </div>
</div>

