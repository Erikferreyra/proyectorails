    <section id="main"
		<div class="panel panel-primary">
			<div class="panel-heading">
				<h3 class="panel-title">
Listado de usuarios</h3>
			</div>
			<div class="panel-body">
				<table class="table table-striped table-hover">
				<tr>
					<th>Nombre</th>
					<th>Apellido</th>
          			<th>Fecha de nacimiento</th>
				</tr>
				<% @users.each do |user| %>
					<tr>
						<td><%= user.nombre %></td>
						<td>
						<%= user.apellido %>
						</td>
            			<td><%=user.fechanac%></td>

						<td>
							<%= form_for user do |u| %>
								<%= u.hidden_field :premium, value: !user.premium %>
    							<%= u.submit "Cambiar a #{user.premium ? "basico" : "premium"}" %>
							<%end%>
						</td>
					</tr>
				<%end%>,
				</table>
			</div>
		</div>
    </section>






